{
  "id": "clean-landing",
  "name": "Clean Landing Page",
  "description": "A minimal brochure-site starter featuring Astro + Tailwind CSS for static pages, with an optional Cloudflare Worker endpoint for contact forms protected by Turnstile and KV storage.",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Astro Template LLM",
  "tags": ["astro", "tailwind", "cloudflare", "landing-page", "business", "responsive"],
  "capabilities": [
    "responsive-design",
    "seo-optimized", 
    "contact-form",
    "security-headers",
    "performance-optimized",
    "accessibility-ready",
    "typescript-support"
  ],
  "compatibility": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0",
    "astro": "^4.15.0"
  },
  "framework": "astro",
  "styling": "tailwindcss",
  "deployment": ["cloudflare-pages", "cloudflare-workers", "static"],
  "preview": "preview.png",
  "tokens": "tokens.json",
  "entry_point": "src/pages/index.astro",
  "build_command": "pnpm build",
  "dev_command": "pnpm dev",
  "install_command": "pnpm install",
  "features": {
    "contact_form": {
      "enabled": true,
      "provider": "cloudflare-worker",
      "protection": "turnstile"
    },
    "seo": {
      "enabled": true,
      "meta_tags": true,
      "open_graph": true,
      "sitemap": true
    },
    "performance": {
      "lighthouse_score": ">=90",
      "image_optimization": true,
      "css_optimization": true
    },
    "security": {
      "csp_headers": true,
      "security_headers": true,
      "form_protection": true
    }
  },
  "pages": ["home", "about", "services", "contact"],
  "components": [
    "Nav",
    "Footer", 
    "Hero",
    "Features",
    "CTA",
    "FAQ",
    "Button"
  ],
  "created_at": "2024-01-01T00:00:00Z",
  "updated_at": "2024-01-01T00:00:00Z"
}